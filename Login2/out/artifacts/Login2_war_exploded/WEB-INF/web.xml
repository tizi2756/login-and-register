<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>controller.Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/Servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>login_Success</servlet-name>
        <servlet-class>controller.login_Success</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>login_Success</servlet-name>
        <url-pattern>/login_Success</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>login_False</servlet-name>
        <servlet-class>controller.login_False</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>login_False</servlet-name>
        <url-pattern>/login_False</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>set_Register</servlet-name>
        <servlet-class>controller.set_Register</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>set_Register</servlet-name>
        <url-pattern>/set_Register</url-pattern>
    </servlet-mapping>
</web-app>